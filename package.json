{
  "name": "paperless-dedupe",
  "version": "0.3.1",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.29.3",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.15.0"
  },
  "scripts": {
    "dev": "pnpm --filter @paperless-dedupe/web dev",
    "build": "pnpm --filter @paperless-dedupe/core build && pnpm --filter @paperless-dedupe/sdk build && pnpm --filter @paperless-dedupe/cli build && pnpm --filter @paperless-dedupe/web build",
    "check": "pnpm --filter @paperless-dedupe/core check && pnpm --filter @paperless-dedupe/sdk check && pnpm --filter @paperless-dedupe/cli check && pnpm --filter @paperless-dedupe/web check",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "pnpm --filter @paperless-dedupe/core test && pnpm --filter @paperless-dedupe/sdk test",
    "cli": "node --import tsx/esm packages/cli/src/bin.ts",
    "test:e2e": "pnpm --filter @paperless-dedupe/web test:e2e"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "8.55.1-alpha.4",
    "@typescript-eslint/parser": "8.55.1-alpha.4",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.15.0",
    "globals": "^17.3.0",
    "prettier": "~3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript-eslint": "8.55.1-alpha.4",
    "vitest": "^4.0.18"
  }
}
